{
  "name": "home_page",
  "version": "1.0.0",
  "description": "parcel template",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "parcel ./index.html --lazy --open chrome --no-cache",
    "build": "rimraf dist/* && parcel build ./index.html --public-url ./",
    "build:analyze": "parcel build ./index.html --reporter @parcel/reporter-bundle-analyzer",
    "build:compatible": "parcel build ./index.html --reporter @parcel/reporter-bundle-buddy --outputFS dist2"
  },
  "keywords": [],
  "author": "IOLOII",
  "license": "ISC",
  "dependencies": {
    "@element-plus/icons-vue": "^2.0.10",
    "axios": "^1.1.3",
    "echarts": "^5.4.0",
    "element-plus": "^2.2.20",
    "gdt-jsapi": "^1.9.39",
    "js-cookie": "^3.0.1",
    "normalize.css": "^8.0.1",
    "number-precision": "^1.6.0",
    "only-allow": "^1.1.1",
    "parcel": "^2.7.0",
    "qs": "^6.11.0",
    "seamscroll": "^0.0.12",
    "tiny-emitter": "^2.1.0",
    "vue": "^3.2.41",
    "vue-echarts": "^6.2.3",
    "vue-router": "^4.1.6",
    "vuex": "^4.1.0",
    "vuex-persistedstate": "^4.1.0"
  },
  "alias": {
    "@/*": "/src/$1"
  },
  "devDependencies": {
    "@parcel/config-default": "^2.7.0",
    "@parcel/reporter-bundle-analyzer": "^2.7.0",
    "@parcel/reporter-bundle-buddy": "^2.7.0",
    "@parcel/transformer-sass": "^2.7.0",
    "@parcel/transformer-vue": "^2.7.0",
    "buffer": "^6.0.3",
    "parcel-reporter-static-files-copy": "^1.4.0",
    "process": "^0.11.10",
    "rimraf": "^3.0.2"
  },
  "staticFiles": {
    "staticPath": "static",
    "staticOutPath": "static"
  },
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "engines": {
    "node": "14.20.0 || >= 14"
  },
  "targets": {
    "main": false,
    "default": {
      "context": "browser",
      "sourceMap": false
    }
  }
}